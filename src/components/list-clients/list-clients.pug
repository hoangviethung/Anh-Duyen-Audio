section.list-clients
	.container
		.row
			.col-12
				h2 Khách hàng của chúng tôi:
			.col-12
				.clients-slider
					.swiper-container
						.swiper-wrapper
							-for(i=1;i<6;i++)
								.swiper-slide
									.box-img
										a(href="#")
											img(src="./img/list-clients/"+i+".png", alt="")
					.swiper-button-prev
					.swiper-button-next